{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"types": ["@types/firefox-webext-browser"],
		"jsx": "preserve",
		"jsxImportSource": "solid-js"
	},
	"include": [
		"./src/**/*.ts",
		"./src/**/*.tsx"
	],
}
